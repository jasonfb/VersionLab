<turbo-frame id="onboarding_form">
  <div class="alert alert-warning mb-3">
    <strong><%= email %></strong> is already registered.
    Please ask your organization's admin to add you.
  </div>

  <div class="d-grid gap-2">
    <%= link_to "Log in instead", new_user_session_path, class: "btn btn-primary" %>
    <%= link_to "Try a different email", onboarding_path, class: "btn btn-outline-secondary" %>
  </div>
</turbo-frame>
